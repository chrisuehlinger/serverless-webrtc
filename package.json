{
  "name": "serverless-webrtc",
  "version": "1.0.5",
  "description": "A terminal text chat client for webrtc datachannels between node and browsers.",
  "author": "Chris Ball <chris@printf.net> (http://printf.net/)",
  "license": "MIT",
  "main": "serverless-webrtc.js",
  "dependencies": {
    "ansi": "0.3.0",
    "wrtc": "~0.0.29"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/chrisuehlinger/serverless-webrtc"
  },
  "keywords": [
    "webrtc",
    "chat"
  ],
  "bugs": {
    "url": "https://github.com/chrisuehlinger/serverless-webrtc/issues"
  },
  "bin": {
    "serverless-webrtc": "serverless-webrtc.js"
  },
  "preferGlobal": true,
  "homepage": "https://github.com/chrisuehlinger/serverless-webrtc",
  "devDependencies": {
    "browser-sync": "^2.26.13"
  },
  "scripts": {
    "serve": "browser-sync start --server --port 9191 --files=\"./**/*.js\" --no-ghost-mode",
    "serve:ssl": "browser-sync start --server --port 9191 --files=\"./**/*.js\" --no-ghost-mode --https",
    "start": "npm run serve"
  }
}
